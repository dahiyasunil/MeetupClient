[
  {
    "version": 2,
    "builds": [{ "src": "index.js", "use": "@vercel/node" }],
    "rewrites": [{ "source": "/(.*)", "destination": "index.js" }]
  },
  {
    "version": 2,
    "builds": [
      {
        "src": "src/main.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
]
